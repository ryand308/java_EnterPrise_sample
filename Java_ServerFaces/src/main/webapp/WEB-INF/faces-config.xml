<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	
	<navigation-rule>
		<from-view-id>/student_from2.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createdStudent</from-outcome>
			<to-view-id>/student_response2.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/guessResponse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/index.html</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>guessGame</from-outcome>
			<to-view-id>/guessNumber.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
